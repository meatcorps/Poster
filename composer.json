        "twig/twig": "^1.32",
        "doctrine/dbal": "2.2.2",
        "symfony/twig-bridge": "^3.2",
        "symfony/security": "^3.2",
        "monolog/monolog": "^1.22",
        "symfony/form": "^3.2",
        "symfony/config": "^3.2",
        "symfony/translation": "^3.2",
        "symfony/validator": "^3.2"
    },
    "license": "Free",
